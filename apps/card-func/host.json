{
  "version": "2.0",
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 5,
        "includedTypes": "PageView;Trace;Dependency",
        "excludedTypes": "Exception;Event;CustomEvent;Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": ""
    },
    "durableTask": {
      "hubName": "FunctionsTemplateHub"
    },
    "queues": {
      "maxPollingInterval": "00:01:00",
      "visibilityTimeout": "00:05:00",
      "batchSize": 1,
      "maxDequeueCount": 10,
      "newBatchThreshold": 0
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  }
}
